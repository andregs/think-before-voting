{
  "name": "think-before-voting",
  "version": "0.0.1",
  "author": "Andr√© Gomes <andregs@gmail.com> (http://andregs.com/)",
  "description": "Package for the Think Before Voting application",
  "main": "./server/server.js",
  "engines": {
    "node": "~6.3.0",
    "npm": "~3.10.0"
  },
  "license": "Apache-2.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/andregs/think-before-voting.git"
  },
  "scripts": {
    "db:create": "arangosh --server.authentication false --javascript.execute script/create-db.script.js",
    "db:drop": "arangosh --server.authentication false --javascript.execute-string db._dropDatabase('think')",
    "nodemon": "nodemon --watch server",
    "nodemon:debug": "nodemon --debug-brk --watch server",
    "dev": "npm-run-all --silent -p tsc:watch nodemon",
    "dev:debug": "npm-run-all --silent -p tsc:watch nodemon:debug",
    "prestart": "npm run build",
    "start": "node server/server.js",
    "tsc": "npm-run-all -p tsc:client tsc:server",
    "tsc:client": "tsc -p client",
    "tsc:server": "tsc -p server",
    "tsc:watch": "npm-run-all -p tsc:client:watch tsc:server:watch",
    "tsc:client:watch": "tsc -w -p client",
    "tsc:server:watch": "tsc -w -p server",
    "lint": "tslint server/**/*.ts client/**/*.ts",
    "prebuild": "npm run test",
    "build": "npm run tsc",
    "pretest": "npm run lint",
    "test": "echo \"TODO: testing...\"",
    "typings:install": "typings install",
    "postinstall": "npm-run-all -p typings:install db:create"
  },
  "devDependencies": {
    "nodemon": "1.10.2",
    "npm-run-all": "3.1.0",
    "tslint": "3.15.1",
    "typescript": "2.0.2",
    "typings": "1.3.3"
  },
  "dependencies": {
    "@angular/common": "2.0.0-rc.6",
    "@angular/compiler": "2.0.0-rc.6",
    "@angular/core": "2.0.0-rc.6",
    "@angular/forms": "2.0.0-rc.6",
    "@angular/http": "2.0.0-rc.6",
    "@angular/platform-browser": "2.0.0-rc.6",
    "@angular/platform-browser-dynamic": "2.0.0-rc.6",
    "@angular/router": "3.0.0-rc.2",
    "@angular/upgrade": "2.0.0-rc.6",
    "@angular2-material/button": "2.0.0-alpha.8-1",
    "@angular2-material/button-toggle": "2.0.0-alpha.8-1",
    "@angular2-material/card": "2.0.0-alpha.8-1",
    "@angular2-material/checkbox": "2.0.0-alpha.8-1",
    "@angular2-material/core": "2.0.0-alpha.8-1",
    "@angular2-material/grid-list": "2.0.0-alpha.8-1",
    "@angular2-material/icon": "2.0.0-alpha.8-1",
    "@angular2-material/input": "2.0.0-alpha.8-1",
    "@angular2-material/list": "2.0.0-alpha.8-1",
    "@angular2-material/menu": "2.0.0-alpha.8-1",
    "@angular2-material/progress-bar": "2.0.0-alpha.8-1",
    "@angular2-material/progress-circle": "2.0.0-alpha.8-1",
    "@angular2-material/radio": "2.0.0-alpha.8-1",
    "@angular2-material/sidenav": "2.0.0-alpha.8-1",
    "@angular2-material/slide-toggle": "2.0.0-alpha.8-1",
    "@angular2-material/slider": "2.0.0-alpha.8-1",
    "@angular2-material/tabs": "2.0.0-alpha.8-1",
    "@angular2-material/toolbar": "2.0.0-alpha.8-1",
    "@angular2-material/tooltip": "2.0.0-alpha.8-1",
    "angular2-jwt": "0.1.21",
    "arangojs": "5.0.3",
    "body-parser": "1.15.2",
    "cerialize": "0.1.10",
    "core-js": "^2.4.0",
    "express": "4.14.0",
    "express-handlebars": "3.0.0",
    "express-jwt": "5.0.0",
    "hammerjs": "2.0.8",
    "lodash": "4.15.0",
    "ng2-translate": "2.4.3",
    "normalize.css": "4.2.0",
    "reflect-metadata": "0.1.8",
    "rxjs": "5.0.0-beta.11",
    "serve-favicon": "2.3.0",
    "systemjs": "0.19.38",
    "zone.js": "0.6.17"
  }
}
