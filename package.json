{
  "name": "think-before-voting",
  "version": "0.0.1",
  "author": "Andr√© Gomes <andregs@gmail.com> (http://andregs.com/)",
  "description": "Package for the Think Before Voting application",
  "main": "./server/server.js",
  "engines": {
    "node": "~6.3.0",
    "npm": "~3.10.0"
  },
  "license": "Apache-2.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/andregs/think-before-voting.git"
  },
  "scripts": {
    "db:create": "arangosh --javascript.execute script/create-db.script.js",
    "db:drop": "arangosh --javascript.execute-string db._dropDatabase('think')",
    "nodemon": "nodemon --watch server",
    "nodemon:debug": "nodemon --debug-brk --watch server",
    "dev": "npm-run-all --silent -p tsc:watch nodemon",
    "dev:debug": "npm-run-all --silent -p tsc:watch nodemon:debug",
    "prestart": "npm run build",
    "start": "node server/server.js",
    "tsc": "npm-run-all -p tsc:client tsc:server",
    "tsc:client": "tsc -p client",
    "tsc:server": "tsc -p server",
    "tsc:watch": "npm-run-all -p tsc:client:watch tsc:server:watch",
    "tsc:client:watch": "tsc -w -p client",
    "tsc:server:watch": "tsc -w -p server",
    "lint": "tslint server/**/*.ts client/**/*.ts",
    "prebuild": "npm run test",
    "build": "npm run tsc",
    "pretest": "npm run lint",
    "test": "echo \"TODO: testing...\"",
    "typings:install": "typings install",
    "postinstall": "npm-run-all -p typings:install"
  },
  "devDependencies": {
    "nodemon": "1.10.0",
    "npm-run-all": "2.3.0",
    "tslint": "3.14.0",
    "typescript": "1.8.10",
    "typings": "1.3.2"
  },
  "dependencies": {
    "@angular/common": "2.0.0-rc.5",
    "@angular/compiler": "2.0.0-rc.5",
    "@angular/core": "2.0.0-rc.5",
    "@angular/forms": "0.3.0",
    "@angular/http": "2.0.0-rc.5",
    "@angular/platform-browser": "2.0.0-rc.5",
    "@angular/platform-browser-dynamic": "2.0.0-rc.5",
    "@angular/router": "3.0.0-rc.1",
    "@angular/upgrade": "2.0.0-rc.5",
    "@angular2-material/button": "2.0.0-alpha.7",
    "@angular2-material/card": "2.0.0-alpha.7",
    "@angular2-material/core": "2.0.0-alpha.7",
    "@angular2-material/grid-list": "2.0.0-alpha.7",
    "@angular2-material/icon": "2.0.0-alpha.7",
    "@angular2-material/input": "2.0.0-alpha.7",
    "@angular2-material/list": "2.0.0-alpha.7",
    "@angular2-material/menu": "2.0.0-alpha.7",
    "@angular2-material/progress-bar": "2.0.0-alpha.7",
    "@angular2-material/radio": "2.0.0-alpha.7",
    "@angular2-material/sidenav": "2.0.0-alpha.7",
    "@angular2-material/tabs": "2.0.0-alpha.7",
    "@angular2-material/toolbar": "2.0.0-alpha.7",
    "@angular2-material/tooltip": "2.0.0-alpha.7",
    "angular2-jwt": "0.1.18",
    "arangojs": "5.0.3",
    "body-parser": "1.15.2",
    "cerialize": "0.1.9",
    "core-js": "^2.4.0",
    "express": "4.14.0",
    "express-jwt": "3.4.0",
    "hammerjs": "2.0.8",
    "lodash": "4.14.2",
    "normalize.css": "4.2.0",
    "reflect-metadata": "0.1.5",
    "rxjs": "5.0.0-beta.6",
    "serve-favicon": "2.3.0",
    "systemjs": "0.19.36",
    "zone.js": "0.6.12"
  }
}
